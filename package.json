{
  "name": "tetris_royale",
  "version": "0.0.1",
  "author": "aempisse",
  "license": "MIT",
  "scripts": {
    "srv-dev": "clear && babel-watch -w src src/server/server.js",
    "client-dev": "clear && webpack-dev-server --mode development --progress --colors --hot --inline",
    "srv-dist": "babel src/server --out-dir dist && node dist/server/server.js",
    "client-dist": "webpack --mode production --progress --colors",
    "test": "mocha --recursive --require babel-core/register --reporter spec",
    "coverage": "NODE_ENV=test nyc --include src --all -r text -r lcov -r text --require babel-core/register mocha "
  },
  "dependencies": {
    "express": "^4.16.3",
    "immutable": "^3.8.2",
    "lodash": "^4.17.10",
    "react": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.1.1",
    "socket.io-client": "^2.1.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.2",
    "chai": "^3.5.0",
    "chai-as-promised": "^7.1.1",
    "chai-equal-jsx": "^1.0.9",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "style-loader": "^0.22.1",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2"
  }
}
